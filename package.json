{
  "name": "backendantonychatbot",
  "version": "1.0.0",
  "description": "Backend devlab chatbot for antony",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "uf": "node ./src/fixtures/userFixture.js",
    "of": "node ./src/fixtures/offreFixture.js",
    "allf": "node ./src/fixtures/userFixture.js && node ./src/fixtures/offreFixture.js",
    "purge": "npx prisma migrate reset --force && npm run allf",

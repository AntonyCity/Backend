    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.71.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "prisma": "^5.22.0"
